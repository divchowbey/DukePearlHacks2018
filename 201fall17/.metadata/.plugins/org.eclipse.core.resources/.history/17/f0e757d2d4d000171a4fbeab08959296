public class App {
	public static void main(String[] args) {
		boolean exists = true;
		while (exists) {
			System.out.print("f");
			exists = false;
			System.out.print("g");
			for (Node find : current.children.values()) {
				if (find.myWeight == myRoot.mySubtreeMaxWeight) {
					current = find;
					exists = true;
				}
			}
		}
	}
}
